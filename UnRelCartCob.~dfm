  object IBQuery_Pendentes: TIBQuery
    Database = Dm.IBDatabase
    Transaction = Dm.IBTransaction
    Active = True
    DataSource = FrmFaturas.Ds_Vendas
    SQL.Strings = (
      'select * from CARNE'
      'WHERE (NUMERO_CONTRATO = :NUMERO_CONTRATO)'
      'order by NUMERO_PARCELA')
    Left = 78
    Top = 273
    ParamData = <
      item
        DataType = ftString
        Name = 'NUMERO_CONTRATO'
        ParamType = ptUnknown
        Size = 11
      end>
